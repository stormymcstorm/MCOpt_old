{
  "cache": "pipeline_out",
  "datasets": {
    "sinusoidal": {
      "type": "gen",
      "shape": [100, 100],
      "layers": [
        {
          "type": "sinusoidal",
          "weight": 0.5,
          "args": {"npeaks": 3}
        },
        {
          "type": "distance",
          "weight": -0.5
        },
        {
          "type": "noise",
          "args": {"random_state": 42, "scale": 0.1}
        }
      ],
      "filters": [
        {"type": "warp", "args": {"scale_factor": 50}}
      ]
    },
    "sinusoidal_noisy": {
      "type": "gen",
      "shape": [100, 100],
      "layers": [
        {
          "type": "sinusoidal",
          "weight": 0.5,
          "args": {"npeaks": 3}
        },
        {
          "type": "distance",
          "weight": -0.5
        },
        {
          "type": "noise",
          "args": {"random_state": 42, "scale": 0.25}
        }
      ],
      "filters": [
        {"type": "warp", "args": {"scale_factor": 50}}
      ]
    },
    "tropoause": {
      "type": "load",
      "src": "https://www.dropbox.com/s/tscfc32bskuotiu/Tropoause-VISContest.zip?dl=1",
      "frames": "Tropoause-VISContest_*.vtk"
    },
    "heated_cylinder": {
      "type": "load",
      "src": "https://www.dropbox.com/s/p8dxv8vlwtaxdgk/heatedcylinder-800-899.zip?dl=1",
      "frames": "data_*.vtp"
    },
    "wind": {
      "type": "load",
      "src": "data/wind.zip",
      "frames": "wind*.vti"
    },
    "red_sea": {
      "type": "load",
      "src": "data/redSea.zip",
      "frames": "redSeaVelocity*.vti"
    },
    "navier_stokes": {
      "type": "load",
      "src": "data/Navier-Stokes.zip",
      "frames": "speed*.vti"
    },
    "tangaroa": {
      "type": "load",
      "src": "data/tangaroa-dataset-50-200.zip",
      "frames": "data_*.vti",
      "filters": [
        {
          "type": "box_clip", 
          "args": {
            "xmin": -0.25,
            "xmax": 0.75,
            "ymin": -0.3,
            "ymax": 0.3,
            "zmin": -0.5,
            "zmax": -0.49
          }
        }
      ]
    }
  },
  "complexes": {
    "sinusoidal": {
      "dataset": "sinusoidal",
      "persistence_threshold": 0.1
    },
    "sinusoidal_noisy": {
      "dataset": "sinusoidal_noisy",
      "persistence_threshold": 0.1
    },
    "tropoause": {
      "dataset": "tropoause",
      "persistence_threshold": 2.5,
      "field_name": "trop_1"
    },
    "heated_cylinder": {
      "dataset": "heated_cylinder",
      "persistence_threshold": 0.05,
      "field_name": "velocityMagnitude"
    },
    "wind": {
      "dataset": "wind",
      "persistence_threshold": 2.5
    },
    "red_sea": {
      "dataset": "red_sea",
      "persistence_threshold": 0.01
    },
    "navier_stokes": {
      "dataset": "navier_stokes",
      "persistence_threshold": 0.1
    },
    "tangaroa": {
      "dataset": "tangaroa",
      "persistence_threshold": 0.1,
      "field_name": "velocityMagnitude"
    }
  }
}