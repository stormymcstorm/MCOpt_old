{
  "cache": "pipeline_out",
  "downloads": {
    "heated_cylinder": {
      "url": "https://github.com/stormymcstorm/MCOpt/releases/download/v0.5.0/heatedcylinder-800-899.zip",
      "frame_pattern": "data_*.vtp"
    },
    "tropopause": {
      "url": "https://github.com/stormymcstorm/MCOpt/releases/download/v0.5.0/tropoause-VISContest.zip",
      "frame_pattern": "Tropoause-VISContest_*.vtk"
    },
    "wind": {
      "url": "https://github.com/stormymcstorm/MCOpt/releases/download/v0.5.0/wind.zip",
      "frame_pattern": "wind*.vti"
    },
    "red_sea": {
      "url": "https://github.com/stormymcstorm/MCOpt/releases/download/v0.5.0/redSea.zip",
      "frame_pattern": "redSeaVelocity*.vti"
    },
    "navier_stokes": {
      "url": "https://github.com/stormymcstorm/MCOpt/releases/download/v0.5.0/Navier-Stokes.zip",
      "frame_pattern": "speed*.vti"
    },
    "tangaroa": {
      "url": "https://github.com/stormymcstorm/MCOpt/releases/download/v0.5.0/tangaroa-dataset-50-200.zip",
      "frame_pattern": "data_*.vti"
    }
  },
  "datasets": {
    "heated_cylinder": {
      "type": "load",
      "download": "heated_cylinder"
    },
    "tropopause": {
      "type": "load",
      "download": "tropopause",
      "filters": [
        {
          "type": "translate",
          "args": {
            "dx": 180,
            "dy": 87
          }
        }
      ]
    },
    "wind": {
      "type": "load",
      "download": "wind"
    },
    "red_sea": {
      "type": "load",
      "download": "red_sea"
    },
    "navier_stokes": {
      "type": "load",
      "download": "navier_stokes"
    },
    "tangaroa": {
      "type": "load",
      "download": "tangaroa",
      "filters": [
        {
          "type": "box_clip", 
          "args": {
            "xmin": -0.25,
            "xmax": 0.75,
            "ymin": -0.3,
            "ymax": 0.3,
            "zmin": -0.5,
            "zmax": -0.49
          }
        },
        {
          "type": "translate",
          "args": {
            "dx": 0.25,
            "dy": 0.3
          }
        }
      ]
    },
    "sinusoidal": {
      "type": "gen",
      "shape": [100, 100],
      "frames": [
        [
          {
            "type": "sinusoidal",
            "weight": 0.5,
            "args": {"npeaks": 3}
          },
          {
            "type": "distance",
            "weight": -0.5
          },
          {
            "type": "noise",
            "args": {"random_state": 42, "scale": 0.1}
          }
        ],
        [
          {
            "type": "sinusoidal",
            "weight": 0.5,
            "args": {"npeaks": 3}
          },
          {
            "type": "distance",
            "weight": -0.5
          },
          {
            "type": "noise",
            "args": {"random_state": 42, "scale": 0.25}
          }
        ]
      ],
      "filters": [
        {"type": "warp", "args": {"scale_factor": 50}}
      ]
    }
  },
  "complexes": {
    "sinusoidal": {
      "dataset": "sinusoidal",
      "persistence_threshold": 0.1
    },
    "leaf": {
      "dataset": "leaf",
      "persistence_threshold": 0.1
    },
    "tropopause": {
      "dataset": "tropopause",
      "persistence_threshold": 2.5,
      "field_name": "trop_1"
    },
    "heated_cylinder": {
      "dataset": "heated_cylinder",
      "persistence_threshold": 0.15,
      "field_name": "velocityMagnitude"
    },
    "wind": {
      "dataset": "wind",
      "persistence_threshold": 2.5
    },
    "red_sea": {
      "dataset": "red_sea",
      "persistence_threshold": 0.01
    },
    "navier_stokes": {
      "dataset": "navier_stokes",
      "persistence_threshold": 0.1
    },
    "tangaroa": {
      "dataset": "tangaroa",
      "persistence_threshold": 0.1,
      "field_name": "velocityMagnitude"
    }
  },
  "graphs": {
    "sinusoidal": {
      "complex": "sinusoidal",
      "sample_rate": 10
    },
    "heated_cylinder": {
      "complex": "heated_cylinder",
      "sample_rate": 25
    },
    "red_sea": {
      "complex": "red_sea",
      "sample_rate": 5
    },
    "wind": {
      "complex": "wind",
      "sample_rate": 5
    },
    "navier_stokes": {
      "complex": "navier_stokes",
      "sample_rate": 5
    },
    "tropopause": {
      "complex": "tropopause",
      "sample_rate": 15,
      "sample_mode": "geo"
    },
    "tangaroa": {
      "complex": "tangaroa",
      "sample_rate": 10
    }
  },
  "figures": {
    "heated_cylinder_graphs": {
      "type": "graphs",
      "graph": "heated_cylinder:::10",
      "title": "Heated Cylinder Graphs",
      "figsize": [5, 2],
      "cmap": "cool",
      "fontsize": 80
    },
    "tropopause_graphs": {
      "type": "graphs",
      "graph": "tropopause:::3",
      "title": "Tropopause Graphs",
      "figsize": [6, 2],
      "cmap": "cool",
      "fontsize": 80
    },
    "wind_graphs": {
      "type": "graphs",
      "graph": "wind",
      "title": "Wind Graphs",
      "figsize": [7, 2],
      "cmap": "cool",
      "fontsize": 80
    },
    "red_sea_graphs": {
      "type": "graphs",
      "graph": "red_sea",
      "title": "Red Sea Graphs",
      "figsize": [5, 2],
      "cmap": "cool",
      "fontsize": 80
    },
    "navier_stokes_graphs": {
      "type": "graphs",
      "graph": "navier_stokes",
      "title": "Navier Stokes Graphs",
      "figsize": [4, 2],
      "cmap": "cool",
      "fontsize": 80
    },
    "tangaroa_graphs": {
      "type": "graphs",
      "graph": "tangaroa::-1:10",
      "title": "Tangaroa Graphs",
      "figsize": [5, 3],
      "cmap": "cool",
      "fontsize": 80
    }
  }
}